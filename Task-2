public class task2 {

    public static void main(String[] args) {

        int a=15 , b=5;
        int add=a+b;
        int sub=a-b;
        int mul= a*b;
        int div= a/b;

        System.out.println("Additon: " + add);
        System.out.println("Subtraction: " + sub);
        System.out.println("Multiplication: " + mul);
        System.out.println("Division: " + div);
    }


}
